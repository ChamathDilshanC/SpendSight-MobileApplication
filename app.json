{
  "expo": {
    "name": "SpendSight",
    "slug": "SpendSight",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SpendSightLogo.png",
    "scheme": "spendsight",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chamathdilshan.spendsight"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/SpendSightLogo.png"
      },
      "package": "com.chamathdilshan.spendsight",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/SpendSightLogo.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.45937352137-8p3m4nutcplmojnot778jp6rdbdadind",
          "ios": {
            "reservedClientId": "com.googleusercontent.apps.45937352137-8p3m4nutcplmojnot778jp6rdbdadind"
          },
          "android": {
            "requestIdToken": true,
            "requestEmail": true,
            "requestProfile": true
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7f73bd84-e734-48c9-a36f-be738dc29dd3"
      }
    }
  }
}
